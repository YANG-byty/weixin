<!--components/goodsList/goodsList.wxml-->
<view class="product-list">
  <view class="list-child" wx:for="{{goodsList}}" wx:key="index">
    <view class="product-item" wx:for="{{item}}" wx:key="index">
      <view class="product-img">
        <image src="{{item.goods_thumb}}"></image>
      </view>
      <view class="product-title">{{item.title}}</view>
      <view class="product-price">ï¿¥{{item.shop_price}}</view>
    </view>
  </view>
</view>
